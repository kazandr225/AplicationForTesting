<Page x:Class="AplicationForTesting.Pages.ResoultsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AplicationForTesting.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ResoultsPage">

    <Grid>
        <StackPanel Orientation="Vertical" Width="255" Height="220">

            <TextBlock Name="tblScore" Loaded="tblScore_Loaded" Margin="0,0,0,10" Uid="{Binding TotalScore}"></TextBlock>
            <TextBlock Name="tblScoreCount" Loaded="tblScoreCount_Loaded" Margin="0,0,0,10" Uid="{Binding TotalScoreLoaded}"></TextBlock>
            <Button Name="btnRating" Click="btnRating_Click" Height="25" Width="200" Margin="0,0,0,10">Рейтинг</Button>
            <Button Name="btmSave" Click="btnSave_Click" Height="25" Width="200" Margin="0,0,0,0">Закончить работу</Button>
        </StackPanel>
    </Grid>
</Page>
