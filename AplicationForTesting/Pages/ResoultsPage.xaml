<Page x:Class="AplicationForTesting.Pages.ResoultsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AplicationForTesting.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ResoultsPage">

    <Grid>
        <StackPanel Orientation="Vertical" Width="255" Height="220">

            <TextBlock Name="tblScore" Loaded="tblScore_Loaded" Uid="{Binding TotalScore}"></TextBlock>
            <TextBox Name="tbScore" Width="255" Height="25"></TextBox>
            <TextBox Name="tbResult" Width="255" Height="25"  Margin="0,5,0,10"></TextBox>
            <Button Name="btmSave" Click="btnSave_Click">Сохранить результат</Button>
        </StackPanel>
    </Grid>
</Page>
